        "# ------------------------------------------------------------------------------\n",
        "# 1. Загрузка зданий и подготовка границы в проекции WGS84 (EPSG:4326)\n",
        "# ------------------------------------------------------------------------------\n",
        "boundary_4326 = boundary.to_crs(epsg=4326)\n",
        "\n",
        "# Загружаем только полигональные здания в пределах границы\n",
        "bld_poly = ox.geometries_from_polygon(boundary_4326.unary_union, tags={\"building\": True})\n",
        "bld_poly = bld_poly.to_crs(city_model.crs).reset_index(drop=True)\n",
        "bld_poly = bld_poly[bld_poly.geometry.type.isin(['Polygon', 'MultiPolygon'])]\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# 2. Обработка этажности зданий\n",
        "# ------------------------------------------------------------------------------\n",
        "# Извлекаем количество этажей из тега building:levels\n",
        "bld_poly[\"number_of_floors\"] = pd.to_numeric(bld_poly.get(\"building:levels\"), errors='coerce')\n",
        "\n",
        "# Если этажность не указана — считаем, что 1 этаж\n",
        "bld_poly[\"number_of_floors\"] = bld_poly[\"number_of_floors\"].fillna(1).round().astype(int)\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# 3. Расчёт строительных метрик\n",
        "# ------------------------------------------------------------------------------\n",
        "# Площадь застройки (в плане)\n",
        "bld_poly[\"footprint_area\"] = bld_poly.geometry.area\n",
        "\n",
        "# Общая строительная площадь (footprint × этажность)\n",
        "bld_poly[\"build_floor_area\"] = bld_poly[\"footprint_area\"] * bld_poly[\"number_of_floors\"]\n",
        "\n",
        "# Определяем, какие здания считаем жилыми\n",
        "residential_tags = ['residential', 'apartments', 'house', 'detached']\n",
        "\n",
        "# Жилая площадь (80% от общей площади для жилых зданий)\n",
        "bld_poly[\"living_area\"] = np.where(\n",
        "    bld_poly.get(\"building\").isin(residential_tags),\n",
        "    0.8 * bld_poly[\"build_floor_area\"],\n",
        "    0\n",
        ")\n",
        "\n",
        "# Нежилая площадь — остаток от общей\n",
        "bld_poly[\"non_living_area\"] = bld_poly[\"build_floor_area\"] - bld_poly[\"living_area\"]\n",
        "\n",
        "# Оценка населения: в среднем около 48 чел. на этаж согласно источникам\n",
        "bld_poly[\"population\"] = np.where(\n",
        "    bld_poly[\"living_area\"] > 0,\n",
        "    bld_poly[\"number_of_floors\"] * 48,\n",
        "    0\n",
        ")\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# 4. Обновление модели города новыми данными о зданиях\n",
        "# ------------------------------------------------------------------------------\n",
        "\n",
        "# Приводим к числовому виду\n",
        "bld_poly[\"number_of_floors\"] = pd.to_numeric(bld_poly.get(\"building:levels\"), errors='coerce')\n",
        "\n",
        "# Заменяем NaN на 1\n",
        "bld_poly[\"number_of_floors\"] = bld_poly[\"number_of_floors\"].fillna(1)\n",
        "\n",
        "# Округляем и переводим в целое число\n",
        "bld_poly[\"number_of_floors\"] = bld_poly[\"number_of_floors\"].round().astype(int)\n",
        "\n",
        "# 💥 Исправляем всё, что оказалось меньше 1 (включая нули и отрицательные значения)\n",
        "bld_poly.loc[bld_poly[\"number_of_floors\"] < 1, \"number_of_floors\"] = 1\n",
        "\n",
        "city_model.update_buildings(bld_poly)\n",
        "\n",
        "# (Повторно на всякий случай — может быть удалено, если выше уже всё корректно)\n",
        "bld_poly[\"number_of_floors\"] = pd.to_numeric(bld_poly.get(\"building:levels\"), errors='coerce')\n",
        "bld_poly[\"number_of_floors\"] = bld_poly[\"number_of_floors\"].fillna(1).round().astype(int)\n",
        "bld_poly.loc[bld_poly[\"number_of_floors\"] < 1, \"number_of_floors\"] = 1\n",
        "bld_poly[\"footprint_area\"] = bld_poly.geometry.area\n",
        "bld_poly[\"build_floor_area\"] = bld_poly[\"footprint_area\"] * bld_poly[\"number_of_floors\"]\n",
        "city_model.update_buildings(bld_poly)\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# 5. Запуск Spacematrix анализа\n",
        "# ------------------------------------------------------------------------------\n",
        "sm = Spacematrix(city_model=city_model)\n",
        "res_sm = sm.calculate()\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# 6. Визуализация результата\n",
        "# ------------------------------------------------------------------------------\n",
        "Spacematrix.plot(res_sm, figsize=(10, 10))"
