        "# Итоговый Score рассчитывается как взвешенная сумма всех факторов\n",
        "candidates['Score'] = (\n",
        "    alpha1 * candidates['Ienergo'] +\n",
        "    alpha2 * candidates['Icultural'] +\n",
        "    alpha3 * candidates['Icost']    +\n",
