        "# ------------------------------------------------------------------------------\n",
        "# Перевод кварталов в метрическую проекцию (EPSG:3857) для точного расчёта площади\n",
        "# ------------------------------------------------------------------------------\n",
        "splitted_blocks_3857 = splitted_blocks.to_crs(epsg=3857)\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# Расчёт площади кварталов в квадратных метрах\n",
        "# ------------------------------------------------------------------------------\n",
        "splitted_blocks_3857['area_m2'] = splitted_blocks_3857.geometry.area\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# Перевод площади в миллионы квадратных метров (для удобства отображения)\n",
        "# ------------------------------------------------------------------------------\n",
        "splitted_blocks_3857['area_mln_m2'] = splitted_blocks_3857['area_m2'] / 1e6\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# Возврат к географической проекции (WGS 84 / EPSG:4326) для отображения на карте\n",
        "# ------------------------------------------------------------------------------\n",
        "splitted_blocks_4326 = splitted_blocks_3857.to_crs(epsg=4326)\n",
        "selected_objects_4326 = selected_objects.to_crs(epsg=4326)\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# Создание интерактивной карты кварталов с градиентом по площади\n",
        "# ------------------------------------------------------------------------------\n",
        "# Используем .explore() из GeoPandas для создания интерактивной Leaflet-карты\n",
        "m = splitted_blocks_4326.explore(\n",
        "    column='area_mln_m2',                 # Окрашиваем по площади (в млн м²)\n",
        "    cmap='Set1',                          # Цветовая палитра\n",
        "    tooltip=['area_mln_m2', 'land_use'],  # Подсказки при наведении: площадь и тип застройки\n",
        "    popup=True,                           # Всплывающее окно при клике\n",
        "    legend=True,                          # Включаем легенду\n",
        "    tiles=\"CartoDB.Positron\"              # Базовая карта (серый фон)\n",
        ")\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# Наложение объектов «плана подсветки» поверх карты\n",
        "# ------------------------------------------------------------------------------\n",
        "# Добавляем на карту отобранные объекты (например, памятники, здания и т.п.)\n",
        "selected_objects_4326.explore(\n",
        "    m=m,                                  # Накладываем на существующую карту\n",
        "    color='red',                          # Цвет маркеров\n",
        "    marker_kwds={\"radius\": 1},            # Размер точек (актуально для точек и центроидов)\n",
        "    tooltip=['name', 'Score'],            # Подсказки: имя и оценка (Score)\n",
        "    popup=True                            # Всплывающее окно\n",
        ")\n",
        "\n",
        "# ------------------------------------------------------------------------------\n",
        "# Вывод финальной карты\n",
        "# ------------------------------------------------------------------------------\n",
        "m  # Leaflet-карта отобразится в Jupyter / JupyterLab"
